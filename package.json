{
  "name": "stamp",
  "version": "0.0.20",
  "description": " A Lotus powered internet cryptomessenger",
  "productName": "Stamp",
  "author": "shammah@cryptonumismatics.org",
  "private": true,
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,js,vue}'",
    "lint:fix": "npm run lint -- --fix",
    "dev": "quasar dev -m electron -- --no-sandbox --disable-setuid-sandbox",
    "build": "quasar build -m electron -b builder",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "generate:protobuffers": "/bin/sh ./generate_protobufs.sh",
    "clean:protobuffers": "git clean -di && git pull",
    "dev-ios": "quasar dev -m capacitor -T ios",
    "build-ios": "quasar build  -m capacitor -T ios",
    "dev-android": "quasar dev -m capacitor -T android",
    "build-android": "quasar build  -m capacitor -T android",
    "build-android-debug": "quasar build  -m capacitor -T android -d",
    "zipalign-android": "zipalign -v -p 4 ./dist/capacitor/android/apk/release/app-release-unsigned.apk ./dist/capacitor/android/apk/release/app-release-unsigned-aligned.apk",
    "build-android-release": "yarn build-android && yarn zipalign-android && apksigner sign --ks release.keystore.jks --out ./dist/capacitor/android/apk/release/app-release.apk ./dist/capacitor/android/apk/release/app-release-unsigned-aligned.apk",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@capacitor/core": "2",
    "@quasar/extras": "^1.11.2",
    "@vue/compiler-sfc": "^3.2.4",
    "atob": "^2.1.2",
    "axios": "^0.21.1",
    "bip39": "^3.0.2",
    "bitcore-lib-xpi": "file:./local_modules/bitcore-lib-xpi",
    "bluebird": "^3.7.2",
    "bn.js": "file:./local_modules/bn.js",
    "btoa": "^1.2.1",
    "core-js": "^3.6.5",
    "dompurify": "^2.3.1",
    "electron-windows-badge": "^1.1.0",
    "electrum-cash": "^2.0.8",
    "google-protobuf": "^3.12.0-rc.1",
    "isomorphic-ws": "^4.0.1",
    "level": "^7.0.0",
    "marked": "^2.0.0",
    "moment": "^2.25.3",
    "node-emoji": "^1.11.0",
    "node-forge": "^0.10.0",
    "qrcode.vue": "^3.3.2",
    "quasar": "^2.1.5",
    "ramda": "^0.27.0",
    "vcf": "^2.1.0",
    "vue-i18n": "^9.1.9",
    "vuex": "^4.0.2",
    "webpack": ">=5",
    "workbox-webpack-plugin": "^6.2.4",
    "worker-loader": "^3.0.8",
    "ws": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.13.0",
    "@capacitor/android": "^2.4.6",
    "@quasar/app": "^3.1.4",
    "@types/atob": "^2.1.2",
    "@types/bluebird": "^3.5.36",
    "@types/btoa": "^1.2.3",
    "@types/google-protobuf": "^3.15.5",
    "@types/node": "^15.14.0",
    "@types/node-forge": "^0.10.3",
    "@types/ramda": "^0.27.44",
    "@types/vcf": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/devtools": "^5.3.4",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "devtron": "^1.4.0",
    "electron": "^13.2.1",
    "electron-builder": "^22.12.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.0.0",
    "eslint-webpack-plugin": "^3.0.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "protoc": "^1.0.4",
    "ts-protoc-gen": "^0.15.0"
  },
  "engines": {
    "node": ">= 14.0.0 <16.0.0",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
